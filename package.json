{
  "name": "taw-3.0",
  "version": "0.0.1",
  "description": "Front-end for TAW.net",
  "license": "UNLICENSED",
  "scripts": {
    "build": "npm run build:clean && npm run build:prod:dll && npm run build:webpack",
    "build:dev:dll": "webpack --config webpack.dev.dll.babel.js --progress",
    "build:prod:dll": "webpack --config webpack.prod.dll.babel.js --progress",
    "build:webpack": "webpack --config webpack.prod.babel.js --progress",
    "build:clean": "rimraf build",
    "clean": "rimraf dist",
    "start": "webpack-dev-server --config webpack.dev.babel.js --hot --inline --progress",
    "start:full": "npm run clean && npm run build:dev:dll && webpack-dev-server --config webpack.dev.babel.js --hot --inline --progress",
    "test": "jest"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  },
  "jest": {
    "notify": true,
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "setupTestFrameworkScriptFile": "./src/test/setup",
    "testPathDirs": [
      "src"
    ],
    "testPathIgnorePatterns": [
      "/src/test/*"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "src/components/Main/index.js"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-jest": "^22.0.4",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "copy-webpack-plugin": "^4.3.1",
    "enzyme": "^3.3.0",
    "eslint": "^4.15.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "jest-cli": "^22.0.4",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.7.2",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.10.0"
  },
  "dependencies": {
    "@cerebral/forms": "^3.0.1",
    "@cerebral/http": "^4.0.1",
    "@cerebral/react": "^3.0.1",
    "@cerebral/router": "^3.0.1",
    "@cerebral/storage": "^4.0.1",
    "cerebral": "^4.0.1",
    "chart.js": "^2.7.1",
    "chartkick": "^2.2.4",
    "colors": "^1.1.2",
    "css-loader": "^0.28.8",
    "detect-browser": "^2.0.0",
    "flatpickr": "^4.2.3",
    "font-awesome": "^4.7.0",
    "function-tree": "^3.0.0",
    "highcharts": "^6.0.4",
    "moment-timezone": "^0.5.14",
    "node-fetch": "^1.7.3",
    "polished": "^1.9.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-big-calendar": "^0.17.0",
    "react-chartkick": "https://github.com/inergy/react-chartkick#refactor-to-react-classes",
    "react-dom": "^16.2.0",
    "react-fa": "^5.0.0",
    "react-flatpickr": "^3.6.3",
    "react-flip-move": "^2.10.1",
    "react-hot-loader": "next",
    "react-icons": "^2.2.7",
    "react-material-responsive-grid": "^1.2.1",
    "react-string-replace": "^0.4.1",
    "react-style-proptype": "^3.1.0",
    "react-tap-event-plugin": "^3.0.2",
    "react-transition-group": "1.2.0",
    "rrule": "^2.2.0",
    "styled-components": "^2.4.0"
  }
}
